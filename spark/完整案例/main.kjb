      <script>#！/bin/bash

###################################################################
#
# shell脚本功能
# 	1、接收、处理参数
#   2、hdfs文件处理
#   3、调用sql脚本
#   4、处理结果文件
#
###################################################################

# 脚本文件路径
base=/home/spark/shell/thirtyDaysPhone
mkdir -p ${base}

# 数据日期,由上一个 step 设置好的变量 DATADATE
data_date=${DATADATE}

file_name=custinfo_${data_date}.txt

# hdfs集群地址头，其中的master可以换成具体的hadoop集群namenode的ip地址或者hostname(使用hostname的前提是在/etc/hosts中做了配置)
hdfs_head="hdfs://vm156:9000"

# 要拷贝的文件原始位置，也就是别的系统推送给我们的文件的存放位置
dir_raw=${hdfs_head}/rawData/

# 要拷贝的文件目标位置,要新建这个目录
dir_dest=${hdfs_head}/rrms/custinfo/${data_date}
hdfs dfs -mkdir -p ${dir_dest}

# 要拷贝的文件
file_raw=${dir_raw}/${file_name}

# 先判断文件是否已经存在，如果不存在则退出脚本并报异常
hadoop fs  -test  -f ${file_raw}   2>&amp;1 >>  ${base}/main_${data_date}.log
if [ $? -ne 0 ];then
	echo "File ${file_raw} does not exist!" >>  ${base}/error.log
	exit 100
fi

# 文件拷贝
hadoop fs -cp ${file_raw} ${dir_dest}   2>&amp;1 >>  ${base}/main_${data_date}.log
if [ $? -ne 0 ];then
	echo "Copy file from ${file_raw} to ${dir_dest} failure!" >>  ${base}/error.log
	exit 1
fi


# 提交spark作业到集群
spark-submit --master yarn --deploy-mode client  --class ccom.zxftech.rrms.MyMain demo.jar ${data_date} ${dir_dest}
if [ $? -ne 0 ];then
	echo "Spark driver executing failure!" >>  ${base}/error.log
	exit 1
fi</script>
